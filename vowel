#include <stdio.h>
 
int main()
{
  char k;
 
  printf("Enter a character\n");
  scanf("%c", &k);
 
  if (k == 'a' || k == 'A' || k == 'e' || k == 'E' || k == 'i' || k == 'I' || k =='o' || k =='O' || k == 'u' || k == 'U')
    printf("%c is a vowel.\n", k);
  else
    printf("%c isn't a vowel.\n", k);
     
  return 0;
}
