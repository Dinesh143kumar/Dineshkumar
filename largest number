#include <stdio.h>
int main()
{
    double a1, a2, a3;
    printf("Enter three numbers: ");
    scanf("%lf %lf %lf", &a1, &a2, &a3);
    if (a1>=a2)
    {
        if(a1>=a3)
            printf("%.2lf is the largest number.", a1);
        else
            printf("%.2lf is the largest number.", a3);
    }
    else
    {
        if(a2>=a3)
            printf("%.2lf is the largest number.", a2);
        else
            printf("%.2lf is the largest number.",a3);
    }
    
    return 0;
}


